{
    "name": "event-transmitter",
    "version": "1.1.2",
    "description": "Transmit events over any stream pipeline.",
    "main": "main.js",
    "browser": {
        "buffertools": "browserify-buffertools"
    },
    "directories": {
        "example": "example",
        "test": "test"
